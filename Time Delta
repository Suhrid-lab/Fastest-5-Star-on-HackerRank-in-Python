import datetime

def time_delta(t1, t2):
    # Define the format string for parsing the timestamps
    time_format = '%a %d %b %Y %H:%M:%S %z'

    # Parse the input strings into datetime objects
    dt1 = datetime.datetime.strptime(t1, time_format)
    dt2 = datetime.datetime.strptime(t2, time_format)

    # Calculate the time difference (timedelta object)
    time_difference = abs(dt1 - dt2)

    # Get the total difference in seconds as an integer
    total_seconds = int(time_difference.total_seconds())

    # Return the result as a string
    return str(total_seconds)

if __name__ == '__main__':
    # Read the number of test cases
    T = int(input())

    for _ in range(T):
        # Read the two timestamp strings for each test case
        t1 = input()
        t2 = input()

        # Call the time_delta function and print the result
        delta = time_delta(t1, t2)
        print(delta)
